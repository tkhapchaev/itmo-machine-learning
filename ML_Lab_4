om.randint(1, 1000)\n","\n","            X_current = X.sample(n=len(X), random_state=seed)\n","            y_current = y.sample(n=len(X), random_state=seed)\n","\n","            tree = sklearn.tree.DecisionTreeClassifier(max_features=features, max_depth=self.max_depth, min_samples_split=self.min_objects_in_leaf)\n","            tree.fit(X_current, y_current)\n","\n","            self.trees.append(tree)\n","\n","    def predict(self, X):\n","        result = []\n","\n","        for tree in self.trees:\n","            result.append(tree.predict(X))\n","\n","        result = pd.DataFrame(data=result)\n","\n","        return round(result.mean())\n","\n","    def features(self):\n","        result = []\n","\n","        for tree in self.trees:\n","            result.append(tree.feature_importances_)\n","\n","        result = pd.DataFrame(data=result)\n","\n","        return result.mean()"],"metadata":{"id":"xjz7CVGB65K-"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["X = churn.iloc[:, :-1]\n","y = churn.iloc[:, -1]\n","\n","X_train, X_test, y_train, y_test = train_test_split(X, y, train_size = 0.7)"],"metadata":{"id":"UyHu13058Gdg"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["random_forest = RandomForest()\n","\n","random_forest.new()\n","random_forest.fit(X_train, y_train)\n","\n","y_predicted = random_forest.predict(X_test)\n","accuracy_score(y_test, y_predicted)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"LidTMBVr664B","executionInfo":{"status":"ok","timestamp":1698233344584,"user_tz":-180,"elapsed":2391,"user":{"displayName":"Timur Khapchaev","userId":"00723514413173892179"}},"outputId":"9dc4a804-c0ea-45a3-a93e-b0c5fe45b750"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["0.855"]},"metadata":{},"execution_count":99}]},{"cell_type":"code","source":["def get_best_hyperparameters():\n","    best_max_ntrees = 0\n","    best_max_depth = 0\n","    best_accuracy_score = 0\n","    best_min_objects_in_leaf = 0\n","\n","    for max_tree in [100, 200, 300, 400, 500, 600]:\n","        for max_depth in range(1, 10):\n","            for min_samples_leaf in range(2, 5):\n","                random_forest = RandomForest()\n","                random_forest.new(max_tree, max_depth, min_samples_leaf)\n","                random_forest.fit(X_train, y_train)\n","\n","                y_predicted = random_forest.predict(X_test)\n","                current_accuracy_score = accuracy_score(y_test, y_predicted)\n","\n","                if current_accuracy_score > best_accuracy_score:\n","                    best_max_ntrees = max_tree\n","                    best_max_depth = max_depth\n","                    best_accuracy_score = current_accuracy_score\n","                    best_min_objects_in_leaf = min_samples_leaf\n","\n","    return best_max_ntrees, best_max_depth, best_min_objects_in_leaf"],"metadata":{"id":"I51aTWVX6_if"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["best_max_ntrees, best_max_depth, best_min_objects_in_leaf = get_best_hyperparameters()"],"metadata":{"id":"xkSbFw7t8XBr"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["best_max_ntrees, best_max_depth, best_min_objects_in_leaf"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"lV5NKHY-oBAp","executionInfo":{"status":"ok","timestamp":1698237217490,"user_tz":-180,"elapsed":265,"user":{"displayName":"Timur Khapchaev","userId":"00723514413173892179"}},"outputId":"22b13b83-9ebb-4e7d-e023-26210d7f093d"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["(100, 9, 3)"]},"metadata":{},"execution_count":103}]},{"cell_type":"code","source":["random_forest = RandomForest()\n","random_forest.new(best_max_ntrees, best_max_depth, best_min_objects_in_leaf)\n","random_forest.fit(X_train, y_train)\n","\n","y_pred = random_forest.predict(X_test)\n","accuracy_score(y_test, y_pred)"],"metadata":{"colab":